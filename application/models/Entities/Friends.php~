<?php

namespace Entities;

/**
 * Entities\Friends
 */
class Friends
{
    /**
     * @var integer $id
     */
    private $id;

    /**
     * @var Entities\User
     */
    private $uid1;

    /**
     * @var Entities\User
     */
    private $uid2;


    /**
     * Get id
     *
     * @return integer $id
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set uid1
     *
     * @param Entities\User $uid1
     */
    public function setUid1(\Entities\User $uid1)
    {
        $this->uid1 = $uid1;
    }

    /**
     * Get uid1
     *
     * @return Entities\User $uid1
     */
    public function getUid1()
    {
        return $this->uid1;
    }

    /**
     * Set uid2
     *
     * @param Entities\User $uid2
     */
    public function setUid2(\Entities\User $uid2)
    {
        $this->uid2 = $uid2;
    }

    /**
     * Get uid2
     *
     * @return Entities\User $uid2
     */
    public function getUid2()
    {
        return $this->uid2;
    }
}